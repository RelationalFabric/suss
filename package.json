{
  "name": "@relational-fabric/suss",
  "type": "module",
  "version": "0.1.0",
  "description": "Relational Propagators for Distributed Causal Logic - Reference implementation of RaCSTS",
  "author": "Relational Fabric",
  "license": "MIT",
  "keywords": [
    "propagators",
    "reactive",
    "state-management",
    "causal",
    "serializable",
    "networks"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "check:all": "npm-run-all check:lint check:types check:test",
    "check:all:fix": "npm-run-all check:lint:fix check:types check:test",
    "check:lint": "eslint .",
    "check:lint:fix": "eslint . --fix",
    "check:test": "vitest run",
    "check:test:watch": "vitest run --watch",
    "check:test:ui": "vitest run --ui",
    "check:types": "npm run check:types:all",
    "check:types:all": "npm-run-all check:types:src check:types:examples",
    "check:types:examples": "tsc --noEmit --project examples/tsconfig.json",
    "check:types:src": "tsc --noEmit",
    "checks": "npm run check:all",
    "dev": "tsx --watch src/index.ts",
    "test": "npm run check:test"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": false
    },
    "typescript": {
      "optional": false
    }
  },
  "dependencies": {
    "immutable": "^5.1.4",
    "vitest": "^4.0.8"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@tsconfig/node-lts": "^22.0.2",
    "eslint": "^9.39.1",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}